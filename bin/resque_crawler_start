#!/usr/bin/env bash

set -e
DATE=$(date +"%Y-%m-%d %H:%M:%S %Z")
SCRIPT_BASEDIR=$(dirname $0)
SCRIPT_BASENAME=$(basename $0)


cd "${SCRIPT_BASEDIR}/.."

export QUEUE=crawler
export PIDFILE=./run/resque_queue_${QUEUE}.pid
export INTERVAL=1
#export COUNT=1
#export BACKGROUND=yes

time rake resque:work
